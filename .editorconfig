# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.{java,rb}]
indent_size = 2

[*.{js,json,jsx,ts,tsx}]
indent_size = 4

[*.md]
max_line_length = off
trim_trailing_whitespace = false
